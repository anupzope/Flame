$include "FVM.lh"
$include "flame.lh"

#include <flame.hh>

namespace flame {

$rule default(thermodynamicModel) {
  $thermodynamicModel = "caloricallyPerfectGas";
}

$rule constraint(caloricallyPerfectGas <- thermodynamicModel) {
  $caloricallyPerfectGas = EMPTY;
  if($thermodynamicModel == "caloricallyPerfectGas") {
    $caloricallyPerfectGas = ~EMPTY;
  }
}

} // end: namespace flame